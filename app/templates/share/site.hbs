{{#if model}}
  {{gallery-share-path site=model.site}}

  {{#photo-swipe items=model.items history=false as |photoswipe|}}
    {{#each model.items as |item index|}}
      {{#link-to 'share/detail' model.site item.info.additional.public_share.shareid}}
        <img src={{item.small}} alt={{item.title}} />
      {{/link-to}}
    {{/each}}
  {{/photo-swipe}}

{{/if}}
{{outlet}}
